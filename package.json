{
  "name": "gulp-angular-boilerplate",
  "version": "1.0.0",
  "description": "Angular boilerplate with gulp and browserify",
  "private": true,
  "engines": {
    "node": ">=0.10.26"
  },
  "devDependencies": {
    "browser-sync": "^2.10.0",
    "connect-modrewrite": "^0.8.2",
