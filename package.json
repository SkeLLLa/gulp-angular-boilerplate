  "scripts": {
    "postinstall": "bower install",
    "pre-build": "npm install",
    "build": "GULP_ENV=production GULP_DEBUG=false gulp build",
    "build:prod": "GULP_ENV=production GULP_DEBUG=false gulp build",
    "build:dev": "GULP_ENV=development GULP_DEBUG=true gulp build",
    "watch:dev": "GULP_ENV=development GULP_DEBUG=true gulp watch"
